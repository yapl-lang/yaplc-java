package org.yapl.main

class Main
	fun main array of string args
		show args
		// Comment

	fun show array of string args returns int i
		for arg in args
			print arg
			++i

class Main {
	fun main(string[] args) {
		show(args)
	}

	fun show(string[] args) returns int i {
		for (arg in args) {
			print(arg)
			++i
		}
	}
}

